<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="unifei.edu.br.techcar" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".LoginActivity"
            android:label="Login"
            android:theme="@style/AppTheme" >
        </activity>
        <activity
            android:name=".CadastroActivity"
            android:label="@string/title_activity_cadastro" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".LoginActivity" />
        </activity>
        <activity
            android:name=".ListAutomovelActivity"
            android:label="@string/title_activity_list_automovel" >
        </activity>
        <activity
            android:name=".CadastroAutomovelActivity"
            android:label="@string/title_activity_cadastro_automovel"
            android:parentActivityName=".ListAutomovelActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="unifei.edu.br.techcar.ListAutomovelActivity" />
        </activity>
        <activity
            android:name=".InfoActivity"
            android:label="@string/title_activity_info"
            android:parentActivityName=".ListAutomovelActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="unifei.edu.br.techcar.ListAutomovelActivity" />
        </activity>
        <activity
            android:name=".EquipeActivity"
            android:label="@string/title_activity_equipe"
            android:parentActivityName=".LoginActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="unifei.edu.br.techcar.LoginActivity" />
        </activity>
        <activity
            android:name=".RelatorioCustosActivity"
            android:label="@string/title_activity_relatorio_custos"
            android:parentActivityName=".ListAutomovelActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="unifei.edu.br.techcar.ListAutomovelActivity" />
        </activity>
    </application>

</manifest>
